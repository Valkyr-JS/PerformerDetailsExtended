{
  "name": "plugin-performer-library-meta",
  "version": "0.1.0",
  "description": "Displays metadata about frequent scene partners, most prominent tags, etc. in your library, on performer pages.",
  "scripts": {
    "build": "npm run compile:ts && npm run compile:scss && npm run copy:yml",
    "compile": "npm run compile:ts && npm run compile:scss",
    "compile:scss": "sass src/performerLibraryMeta.scss dist/performerLibraryMeta.css --no-source-map",
    "compile:ts": "webpack",
    "copy:yml": "node -e \"require('fs').cpSync('./src/performerLibraryMeta.yml', './dist/performerLibraryMeta.yml', {recursive: true});\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Valkyr-JS/plugin-performer-library-meta.git"
  },
  "author": "Valkyr-JS",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Valkyr-JS/plugin-performer-library-meta/issues"
  },
  "homepage": "https://github.com/Valkyr-JS/plugin-performer-library-meta#readme",
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-bootstrap": "^0.32.36",
    "@types/react-dom": "^18.3.0",
    "sass": "^1.77.4",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "webpack": "^5.92.0",
    "webpack-cli": "^5.1.4"
  }
}
